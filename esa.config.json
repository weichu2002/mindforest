{
  "functions": {
    "ai": {
      "handler": "./functions/ai.js",
      "methods": ["POST", "OPTIONS"]
    }
  },
  "routes": [
    {
      "pattern": "/",
      "file": "./index.html"
    },
    {
      "pattern": "/ai",
      "function": "ai"
    },
    {
      "pattern": "/api/ai",
      "function": "ai"
    }
  ],
  "environment": {
    "API_KEY": "你的DeepSeek-API-Key"
  }
}
